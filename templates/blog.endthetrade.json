        "1640078707f8cbb6b7-1": {
          "type": "column",
          "settings": {
            "image": "shopify:\/\/shop_images\/Rectangle_160.png",
            "title": "",
            "text": "<p><strong>Public Awareness <\/strong><\/p><p>To spread the word on the<br\/>campaign offline and <br\/>online to an existing <br\/>community of animal <br\/>lovers and beyond via <br\/>partnerships - charities,<br\/>veterinarians, pet-friendly<br\/>businesses.<\/p>",
