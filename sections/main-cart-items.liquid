{% schema %}
{
  "name": "t:sections.main-cart-items.name",
"class":"cart-page-items cart-main",
  "settings": [
    {
      "type": "checkbox",
      "id": "show_vendor",
      "default": false,
      "label": "t:sections.main-cart-items.settings.show_vendor.label"
    },
{
      "type": "checkbox",
      "id": "show_cart_note",
      "default": false,
      "label": "t:sections.main-cart-footer.settings.show_cart_note.label"
    }
  ],
"blocks": [
{
      "type": "shop_button",
      "name": "Shop buttons",
      "limit": 3,
      "settings": [
        {
          "type": "text",
          "id": "button_label",
          "label": "Button label"
        },
        {
          "type": "url",
          "id": "button_link",
          "label": "Button Link"
        }
      ]
    }
]
}
{% endschema %}
