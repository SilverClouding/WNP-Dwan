<style>

.minibag-mask {
  background: rgba(41, 41, 41, 0.5);
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 99;
  display: none; }

.mini-bag-container {
  position: relative; }

#mini-cart {
  background: #fff;
  top: 0;
  right: 0;
  width: 380px;
  padding: 0;
  position: fixed;
  z-index: 9999;
  height: 100vh; }

.hide-minibag {
  transform: translateX(110%);
  transition: all .5s ease-in-out;
  visibility: hidden;
  width: 0 !important;
  overflow-x: hidden; }

.show-minibag {
  transition: all .5s ease-in-out;
  transform: translateX(0%);
  visibility: visible;
  overflow-x: hidden; }

.overflow-hidden {
  overflow: hidden; }

body.page-your-shopping-cart.overflow-hidden {
  overflow: hidden; }

body.page-your-shopping-cart {
  overflow: unset; }

#mini-cart .count-cross {
  position: relative; }

#mini-cart .count-cross .count {
  color: #161616;
  font-size: 16px;
  font-weight: 400; }

#mini-cart .count-cross .btn {
  position: absolute;
  right: 10px;
  top: 5px;
  z-index: 9;
  cursor: pointer; }

#mini-cart .min-cart-items {
  overflow-y: auto; 
    display: flex;
    justify-content: flex-start;
    flex-wrap: wrap;
    margin: 0 5px;
}

#mini-cart .min-cart-item {
 width:33.333%;
  padding:0 5px;
  margin-bottom: 10px;
}
  
#mini-cart .td-image img {
  width: 100%;
  height: auto; }

.td-image .price {
    font-size: 1.3rem;
    line-height: 2rem;
}

  .td-image h2, .cart-item--remove-link {
    font-size: 1.1rem;
    height: 18px;
    line-height: 1.8rem;
    margin-top: 0px;
    text-transform: capitalize;
    color: #242020;
    margin-bottom:20px;
  }

#mini-cart .info .btn-remove {
  color: #242020;
  font-size: 13px;
  text-transform: uppercase;
  text-decoration: underline;
  margin-left: 15px;
  font-weight: 300;
  margin-top: 6px; }

#mini-cart .subtotal {
  text-align: right;
  padding-right: 5px;
  display: flex;
  justify-content: space-between;
  padding: 1rem;
  width: 100%; }

#mini-cart .subtotal strong {
  font-weight: 500; }

#mini-cart .count, #mini-cart .subtotal, #mini-cart .buttons {
  padding: 10px; }

#mini-cart table tr, #mini-cart .count {
  border-bottom: 1px solid #ccc; }

#mini-cart .quantity-con {
  display: flex;
  margin-top: 11px; }

#mini-cart .quantity-con .quantity-val {
  border: 1px solid #ccc;
  padding: 5px; }

@media (max-width: 767px) {
  div#mini-cart {
    width: 100%; }
}

  a.btn.close-bag svg {
    fill: #5e5c5c;
    width: 24px;
    height: 24px;
    vertical-align: middle;
}
  .bottom-part a.button-custom.button {
    width: 100%;
}
  
  .td-image .buttons-div,  .td-image:hover h2, .td-image:hover .price{
  display:none;
  }
  .td-image:hover .buttons-div{
  display:block;
  }
</style>
   

<div class="mini-cart-con" id="mini__Cart">
 <div class="count-cross">
    <div class="count" count="{{cart.item_count}}">
      {{cart.item_count}} item(s) in your cart
    </div>
    <a id="closeBag" class="btn close-bag pt-0" tabindex="0" href="javascript:void(0)" onclick="HideFunction()" >
      <svg class="svg-inline--fa fa-times fa-w-12" aria-hidden="true" data-fa-replacement="true" data-prefix="fal" data-icon="times" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 384 512"><path fill="currentColor" d="M217.5 256l137.2-137.2c4.7-4.7 4.7-12.3 0-17l-8.5-8.5c-4.7-4.7-12.3-4.7-17 0L192 230.5 54.8 93.4c-4.7-4.7-12.3-4.7-17 0l-8.5 8.5c-4.7 4.7-4.7 12.3 0 17L166.5 256 29.4 393.2c-4.7 4.7-4.7 12.3 0 17l8.5 8.5c4.7 4.7 12.3 4.7 17 0L192 281.5l137.2 137.2c4.7 4.7 12.3 4.7 17 0l8.5-8.5c4.7-4.7 4.7-12.3 0-17L217.5 256z"></path></svg>
    </a>
  </div>
  <div class="bottom-part">
    <div class="subtotal">
      <strong>Subtotal </strong>
      <strong class="amount price">{{ cart.total_price  | money }}</strong>
    </div>

    <div class="buttons">
      <a href="/cart" class="button-custom button">View Cart</a>

    </div>
  </div>
 
  <div class="min-cart-items">

    {% assign allreadyInCart = cart.items | map: "product" | map: "id" | uniq  %}

    {% for item in cart.items %}

    {% assign groupFlag = "" %}

    {% for tag in item.product.tags %}
    {% if tag contains "cart-group" %}
    {% assign groupFlag = tag | split:"-group:" | last %}
    {% endif %}
    {% endfor %}


    <div class="min-cart-item" index="{{forloop.index}}">
      <div class="td-image">
        <img src="{{item.image | img_url: '100x100' }}" />
        <h2>{{item.product.title }}</h2>
         <span class="item-price price">{{ item.price  | money }}</span>
        
        <div class="buttons-div">
          <a onclick="updateItemById('{{ item.id }}','0','cart-items')" href="javascript:void(0)" id="myBtn" ItemId="{{ item.id }}" line="{{forloop.index}}" class="btn-remove cart-item--remove-link" data-cartitem-remove>  {% render 'icon-remove' %}</a>

        </div>
        
      </div>
      {% comment %}
      <div class="info">
        <div class="p-title"> 
          <h2>{{item.product.title }}</h2>
          {% assign removeTitle =  item.product.title | append: " /"%}
          <p>{{ item.variant.title | remove: removeTitle }}</p>
        </div>
        <div class="quantity-con">
          <div class="quantity-val">Qty: {{ item.quantity }}</div>
          <a onclick="updateItemById('{{ item.id }}','0','cart-items')" href="javascript:void(0)" id="myBtn" ItemId="{{ item.id }}" line="{{forloop.index}}" class="btn-remove cart-item--remove-link" data-cartitem-remove>Remove</a>
        </div>


      </div>
      <div class="td-price">
        <span class="item-price price">{{ item.price  | money }}</span>
      </div>
      {% endcomment %}

    </div>	
    {% endfor %}
  </div>



</div>
{% endcomment %}




















