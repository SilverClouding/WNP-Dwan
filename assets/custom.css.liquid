#shopify-section-announcement-bar .announcement-right ul li :focus-visible{
  outline:none;
  box-shadow:none;
}

/* header */
.header{ 
  display:flex;
  justify-content:center;
  padding-top:24px;
  padding-bottom:0px !important;
}
.header .header__heading-link{
  padding:0;
  margin-top:-10px;
}
.header .search-area-con{
  width:59%;
  margin:0 auto; 
}
.header .header__inline-menu{
  margin-top:5px !important;
  margin-left:0;
}
.header .header__inline-menu .list-menu--inline{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.header .header__inline-menu li{
  margin-right:30px;
}
.header .header__inline-menu li.active,
.header .collapsible:hover{
  background-color:transparent !important;
}
.header .header__inline-menu li:last-child{
  margin-right:0;
}
.header .header__inline-menu li:nth-child(5) .header__menu-item{
  color:#F5694D;
}
.header-wrapper--border-bottom{
border-bottom:none !important;
}
.header__inline-menu .header__menu-item{
  font-weight:800;
  font-size:12px;
  color:#fff;
  line-height:18px;
  padding:1.2rem 0 1.3rem;
  border-bottom:2px solid transparent;
}
.header__inline-menu .header__menu-item:hover,
.header__inline-menu .header__menu-item:focus{
   border-bottom:2px solid #F5694D;
}
.customer a:hover, .link:hover{
  text-underline-offset:unset;
}
.header .header__menu-item:hover span{
  text-decoration:unset;
}
.customer .field input:autofill, .customer 
.field input:focus, .customer 
.field input:not(:placeholder-shown), 
.field__input:autofill, .field__input:focus, 
.field__input:not(:placeholder-shown){
  padding:1.2rem 1.5rem .8rem;
}
.header .cart-count-bubble{
  background:#F5694D;
  width:19px;
  height:19px;
  color: #fff;
  font-size: 12px;
  font-weight: 300;
  line-height: 16px;
  top:-5px;
  bottom:unset;
  left:15px;
}
.header__icon, .header__icon--cart .icon {
  height: 30px;
  width: 30px;
}
.header .header__icon--cart{
  margin-right:0;
}
.header .header__icon svg, .header .header__icon--cart .icon svg{
  height: 30px;
  width: 30px;
}
.header .header__icon--whishlist{
  position:relative;
}
.header .header__icon--account svg, .header .header__icon--whishlist svg{
  height: 24px;
  width: 24px;
}
.header .header__icon--account{
  position:relative;
  margin:0 47px 0;
}
.header .header__icon--account:before, .header .header__icon--account:after{
  position: absolute;
  content: '';
  width: 1px;
  height:25px;
  background: #fff;
}
.header .header__icon--account:before{
  left: -22px;
}
.header .header__icon--account:after{
  right: -22px;
}
.header .header__icons{
  flex-direction: column;
}
.header .header__icons-inner{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
}
.header .re-order{
  display: flex;
  justify-content: flex-end;
  color:#F5694D;
  font-size:16px;
  font-weight:800;
  line-height:18px;
  margin-top:31px;
  text-decoration:none;
  align-items: center;
}
.header .re-order:hover, .header .re-order:hover svg{
  color:#fff;
  fill:#fff;
  stroke:#fff;
}
.header .re-order svg{ margin-right:15px;}
 
/* search bar */
.header__search .search .search__input{
  background:#fff;
  -webkit-border-radius:5px;
  -moz-border-radius:5px;
  -o-border-radius:5px;
  -ms-border-radius:5px;
  border-radius:5px;
  box-shadow:none;
  color:#44444D;
  font-size:16px;
  font-weight:300;
  padding:1.2rem 1.2rem .8rem;
  height:55px;
}
.header__search .search .field__button{
  height:5.4rem;
  right:15px;
}
.header .search__button .icon{
  height: 18px;
  width: 18px;
}
.header__search .search .search__button svg{
  color:#44444D;
}
.search-focused{
  width:100%;
}
.header__search{ width:100%;}
.search-modal__form{
  max-width:unset;
}
.search-modal__content{
  width:100%;
  padding:0;
}
.header .customer .field label, .header .field__label{
  font-size:16px;
  line-height:22px;
  font-weight:300;
  color:#44444D;
  top:15px;
  letter-spacing:0;
}
.brand_logos img {
  position: unset!important;
}
@media(min-width:768px){
  .brand_logos.logo-size-con.multicolumn .swiper-slide{
    width:193.167px !important;
    margin-left:0 !important;
  }

}

