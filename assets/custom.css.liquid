}

@font-face {
  font-family: 'Avenir Next LT Pro';
  src: url('AvenirNextLTPro-Regular.eot');
  src: url('AvenirNextLTPro-Regular.eot?#iefix') format('embedded-opentype'),
    url('AvenirNextLTPro-Regular.woff2') format('woff2'),
    url('AvenirNextLTPro-Regular.woff') format('woff'),
    url('AvenirNextLTPro-Regular.ttf') format('truetype'),
    url('AvenirNextLTPro-Regular.svg#AvenirNextLTPro-Regular') format('svg');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}

body{
  font-family: 'Avenir Next LT Pro';
  font-weight:400;
  font-size:12px; 
  line-height:14px;
  color:#44444D;
}
.h0, .h1, .h2, .h3, .h4, .h5, h1, h2, h3, h4, h5{
  font-family: 'Avenir LT Std';
  letter-spacing:0;
}
.h1, h1{
  font-size:24px;
  font-weight:500;
}
.h1, h2{
  font-size:35px;
  font-weight:900;
  color:#153A33;
  line-height:47.81px;
  letter-spacing: 0;
  font-family: 'Avenir LT Std';
  text-transform: capitalize;
}
h3{
  font-size:17px;
  font-weight:400;
}
h4{
  font-size:15px;
  font-weight:500;
}
h5{
  font-size:12px;
  font-weight:500;
}
h6{
  font-size:12px;
  font-weight:400;
}
a, a:active{
  box-shadow:none;
  outline:none;
  border:none;
}
p{
  margin:0;
  padding:0;
  color:#44444D;
  font-size:16px;
  line-height:22px;
  font-weight:300;
  letter-spacing:0;
}
::-webkit-placeholder {
  color: #737373;
}
::-moz-placeholder {
  color: #737373;
}
::-ms-placeholder {
  color: #737373;
}
::placeholder { 
  color: #737373;
  opacity: 1;
}


.button.button--primary{
  background:transparent;
  color:#44444D;
  font-size:14px;
  line-height:16px;
  font-weight:300;
  font-style:normal;
  text-align:center;
  padding:8px 20px;
  box-shadow:none;
  border:1px solid #44444D;
  min-height:unset;
  max-width:unset;
  flex-grow:unset !important;
  opacity:unset;
  cursor:pointer;
  box-shadow:none;
  -webkit-border-radius:2px;
  -o-border-radius:2px;
  -ms-border-radius:2px;
  border-radius:2px;
  letter-spacing:0;
}
.button.button--primary:hover,
.button.button--primary:focus{
  background:#153A33;
  color:#fff;
}
.button.button--secondary{
  background:#153A33;
  color:#fff;
  font-size:15px;
  line-height:16px;
  font-weight:300;
  font-style:normal;
  text-align:center;
  padding:15px 50px;
  box-shadow:none;
  border:1px solid #153A33;
  min-height:unset;
  max-width:unset;
  flex-grow:unset !important;
  opacity:unset;
  cursor:pointer;
  box-shadow:none;
  -webkit-border-radius:2px;
  -o-border-radius:2px;
  -ms-border-radius:2px;
  border-radius:2px;
  letter-spacing:0;
}
.button.button--secondary:hover,
.button.button--secondary:focus{
  background:transparent;
  border:1px solid #44444D;
  color:#44444D;
}


.button:not([disabled]):hover, .customer button:hover, .shopify-challenge__button:hover {
  box-shadow: none;
}

@media(max-width:992px){
  .page-width{
    padding:0 20px;
  }
  .button.button--primary, 
  .button.button--secondary{
    font-size:16px;
    line-height:16px;
    font-weight:500;
    padding:14px 24px;
  }
  p{
    font-size:18px;
  }
}
@media(max-width:360px){
  .button.button--primary, .button.button--secondary{
    padding:12px 15px;
  }
}

/* placeholder  */
::-webkit-placeholder {
  color: #44444D;
}
::-moz-placeholder {
  color: #44444D;
}
::-ms-placeholder {
  color: #44444D;
}
::placeholder { 
  color: #44444D;
}

/*  announcement bar start */
#shopify-section-announcement-bar .announcement-bar__message{
  position:relative;
  margin: 0 auto;
  padding:1.3rem 0;
  display: flex;
  justify-content: center;
  align-items: center;
  width: auto;
  max-width: 59%!important;
}
.announcement-bar .swiper{
  overflow:hidden;
}
/* .announcement-bar__message.h5 svg {margin-right:10px;} */
.announcement-bar__message.h5 svg path {
  fill: #44444D;
}
.announcement-bar__link{
  padding:2px 0 0 0;
}
.announcement-bar__link:hover{
  background:none;
}
.announcement_text_slider {
  overflow: hidden;
}
#shopify-section-announcement-bar .announcement-bar__message p{
  font-family: 'Avenir LT Std';
  font-weight:400;
  font-size:12px;
  color:#44444D;
  line-height:16.39px;
  letter-spacing:0;
  padding-left:10px;
}
#shopify-section-announcement-bar .announcement-bar__message p strong{
  font-weight:900;
}
#shopify-section-announcement-bar .announcement-right{
  position: absolute;
  right: 50px;
  top: 11px;
  display: flex;
  z-index:9;
}
#shopify-section-announcement-bar .announcement-right ul{
  display: flex;
  justify-content: center;
  align-items: center;
  margin:0;
  padding:0;
}
#shopify-section-announcement-bar .announcement-right ul li{
  display: flex;
  justify-content: center;
  align-items: center;
  color:#44444D;
  font-size:12px;
  line-height:14px;
  font-weight:300;
  margin-left:15px;
  letter-spacing:0;
}
#shopify-section-announcement-bar .announcement-right ul li svg{
  margin-right:9px;
}
@media(max-width:767px){
  #shopify-section-announcement-bar .announcement-bar__message{
    max-width: 100% !important;
    margin: 0 auto;
  }
  #shopify-section-announcement-bar .swiper-slide{
    width:100% !important;
  }

}

/* custom select box  */

select {
  outline: 0;
  border: 0;
  box-shadow: none;
  flex: 1;
  color: #44444D;
  background-color: transparent;
  background-image: none;
  cursor: pointer;
  position: relative;
  display: flex;
  overflow: hidden;
}
