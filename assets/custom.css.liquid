/* font family */
@font-face {
  font-family: 'Avenir LT Std';
  src: url('AvenirLTStd-Black.eot');
  src: url('AvenirLTStd-Black.eot?#iefix') format('embedded-opentype'),
    url('AvenirLTStd-Black.woff2') format('woff2'),
    url('AvenirLTStd-Black.woff') format('woff'),
    url('AvenirLTStd-Black.ttf') format('truetype'),
    url('AvenirLTStd-Black.svg#AvenirLTStd-Black') format('svg');
  font-weight: 900;
  font-style: normal;
  font-display: swap;
} 
@font-face {
  font-family: 'Avenir LT Std';
  src: url('AvenirLTStd-Book.eot');
  src: url('AvenirLTStd-Book.eot?#iefix') format('embedded-opentype'),
    url('AvenirLTStd-Book.woff2') format('woff2'),
    url('AvenirLTStd-Book.woff') format('woff'),
    url('AvenirLTStd-Book.ttf') format('truetype'),
    url('AvenirLTStd-Book.svg#AvenirLTStd-Book') format('svg');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
@font-face {
  font-family: 'Avenir LT Std';
  src: url('AvenirLTStd-Roman.eot');
  src: url('AvenirLTStd-Roman.eot?#iefix') format('embedded-opentype'),
    url('AvenirLTStd-Roman.woff2') format('woff2'),
    url('AvenirLTStd-Roman.woff') format('woff'),
    url('AvenirLTStd-Roman.ttf') format('truetype'),
    url('AvenirLTStd-Roman.svg#AvenirLTStd-Roman') format('svg');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}

body{
  font-family: 'Avenir LT Std';
  font-weight:400;
  font-size:12px; 
  line-height:14px;
  color:#44444D;
}
h1, h2, h3, h4, h5, h6{
  font-family: 'Avenir LT Std';
  letter-spacing:0;
}

.h1, h1{
  font-size:24px;
  font-weight:500;
}
h2, .h1{
  font-size:35px;
  font-weight:900;
  color:#153A33;
  line-height:47.81px;
  letter-spacing: 0;
  font-family: 'Avenir LT Std';
}
h3{
  font-size:17px;
  font-weight:400;
}
h4{
  font-size:15px;
  font-weight:500;
}
h5{
  font-size:12px;
  font-weight:500;
}
h6{
  font-size:12px;
  font-weight:400;
}
p{
  margin:0;
  padding:0;
  color:#44444D;
  font-size:16px;
  line-height:22px;
  font-weight:300;
  letter-spacing:0;
}

.button.button--primary{
  background:transparent;
  color:#44444D;
  font-size:14px;
  line-height:16px;
  font-weight:300;
  font-style:normal;
  text-align:center;
  padding:8px 20px;
  box-shadow:none;
  border:1px solid #44444D;
  min-height:unset;
  max-width:unset;
  flex-grow:unset !important;
  opacity:unset;
  cursor:pointer;
  box-shadow:none;
  -webkit-border-radius:2px;
  -o-border-radius:2px;
  -ms-border-radius:2px;
  border-radius:2px;
}
.button.button--primary:hover,
.button.button--primary:focus{
  background:#153A33;
  color:#fff;
}
.button:not([disabled]):hover, .customer button:hover, .shopify-challenge__button:hover {
  box-shadow: none;
}


@media(max-width:992px){
  .page-width{
    padding:0 30px;
  }
}


/* placeholder  */
::-webkit-placeholder {
  color: #44444D;
}
::-moz-placeholder {
  color: #44444D;
}
::-ms-placeholder {
  color: #44444D;
}
::placeholder { 
  color: #44444D;
}


/* mini cart */
.minibag-mask {
  background: rgba(41, 41, 41, 0.5);
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 99;
  display: none; }

.mini-bag-container {
  position: relative; }

#mini-cart {
  background: #fff;
  top: 0;
  right: 0;
  width: 380px;
  padding: 0;
  position: fixed;
  z-index: 9999;
  height: 100vh; transition: all .4s ease-in-out;}

.hide-minibag {
  transform: translateX(110%);
  visibility: hidden;
  width: 0 !important;
  overflow-x: hidden; }

.show-minibag {
  transform: translateX(0%);
  visibility: visible;
  overflow-x: hidden; }

.overflow-hidden {
  overflow: hidden; }

body.page-your-shopping-cart.overflow-hidden {
  overflow: hidden; }

body.page-your-shopping-cart {
  overflow: unset; }

#mini-cart .count-cross {
  position: relative; }

#mini-cart .count-cross .count {
  color: #161616;
  font-size: 16px;
  font-weight: 400; }

#mini-cart .count-cross .btn {
  position: absolute;
  right: 10px;
  top: 5px;
  z-index: 9;
  cursor: pointer; }

#mini-cart .min-cart-items {
  overflow-y: auto; 
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  margin: 0 5px;
}

#mini-cart .min-cart-item {
  width:100%;
  padding:0 5px;
  margin-bottom: 10px;
}

#mini-cart .td-image img {
  width: 100%;
  height: auto; }

.td-image .price {
  font-size: 1.3rem;
  line-height: 2rem;
}

.td-image h2, .cart-item--remove-link {
  font-size: 1.1rem;
  height: 18px;
  line-height: 1.8rem;
  margin-top: 0px;
  text-transform: capitalize;
  color: #242020;
  margin-bottom:20px;
}

#mini-cart .info .btn-remove {
  color: #242020;
  font-size: 13px;
  text-transform: uppercase;
  text-decoration: underline;
  margin-left: 15px;
  font-weight: 300;
  margin-top: 6px; }

#mini-cart .subtotal {
  text-align: right;
  padding-right: 5px;
  display: flex;
  justify-content: space-between;
  padding: 1rem;
  width: 100%; }

#mini-cart .subtotal strong {
  font-weight: 500; }

#mini-cart .count, #mini-cart .subtotal, #mini-cart .buttons {
  padding: 10px; }

#mini-cart table tr, #mini-cart .count {
  border-bottom: 1px solid #ccc; }

#mini-cart .quantity-con {
  display: flex;
  margin-top: 11px; }

#mini-cart .quantity-con .quantity-val {
  border: 1px solid #ccc;
  padding: 5px; }

@media (max-width: 767px) {
  div#mini-cart {
    width: 100%; }
}

a.btn.close-bag svg {
  fill: #5e5c5c;
  width: 24px;
  height: 24px;
  vertical-align: middle;
}
.bottom-part a.button-custom.button {
  width: 100%;
}

.td-image .buttons-div,  .td-image:hover h2, .td-image:hover .price{
  display:none;
}
.td-image:hover .buttons-div{
  display:block;
}





/*  announcement bar start */
